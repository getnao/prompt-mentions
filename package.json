    "@rollup/plugin-typescript": "^12.1.0",
    "@types/react": "^19.2.7",
    "react": "^19.2.3",
    "rollup": "^4.40.0",
    "rollup-plugin-dts": "^6.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.8.0",
    "typescript": "^5.9.3"
